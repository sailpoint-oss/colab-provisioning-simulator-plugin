<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" [<!ENTITY SailPointObject SYSTEM "classpath://sailpoint/object/SailPointObject.hbm.xml">]>
<hibernate-mapping>
    <class name="pro.jakubiak.provisioning.simulator.ProvisioningRecord">
        &SailPointObject;
        <many-to-one name="identity" class="sailpoint.object.Identity"/>
        <property name="applicationName" type="string" length="128"/>
        <property name="nativeIdentity" type="string" length="128"/>
        <property name="operation" type="string" length="128"/>
        <property name="oldValue" type="string" length="128"/>
        <property name="newValue" type="string" length="128"/>
        <property name="identitySupplId" type="string" length="128"/>
        <property name="identityName" type="string" length="128"/>
        <property name="provisioningStatus" type="string" length="128"/>
        <property name="attributeName" type="string" length="128"/>
    </class>
</hibernate-mapping>